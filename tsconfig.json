{
  "compilerOptions": {
    "target": "es5",
    "module": "es6",
    "moduleResolution": "node",
    "sourceMap": true,
    "noImplicitAny": false,
    "removeComments": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "allowJs": true,
  "inlineSourceMap": true,
  "target": "ES5",
  "lib": [
    "DOM",
    "ES2015"
  ],
  "exclude": [
    "node_modules"
  ],
  "awesomeTypescriptLoaderOptions": {}
}
